      </intent-filter>
      <meta-data android:name="unityplayer.UnityActivity" android:value="true" />
    </activity>
    
    <activity 
		android:name="com.mobfox.sdk.interstitialads.InterstitialActivity"
		>
	</activity>

    
  </application>
